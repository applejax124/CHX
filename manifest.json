{
"manifest_version": 2,

"name":"ChromEx",
"description": "The basic boilerplate for creating a working chrome extension.",
"version":"0.0.0.0",

"omnibox":{"keyword": ";"},
"icons":{
  "16": "icons/icon.png"
},


"background": {
  "persistent":false,
	"scripts": [
    "scripts/modules/bookmarks.js",
    "scripts/modules/wikipedia.js",
    "scripts/modules/search/google.js",
    "scripts/modules/search/duckduckgo.js",
    "scripts/modules/google-webapps/mail/gmail.js",
    "scripts/modules/google-webapps/mail/inbox.js",
    "scripts/modules/google-webapps/youtube.js",
    "scripts/modules/google-webapps/drive.js",
    "scripts/modules/google-webapps/keep.js",
    "scripts/modules/google-webapps/maps.js",
    "scripts/modules/google-webapps/calendar.js",
    "scripts/modules/google-webapps/chrome/chrome_settings.js",
    "scripts/modules/google-webapps/chrome/chrome_extensions.js",
    "scripts/background.js"
  ]
},


"permissions":["tabs","bookmarks"]
}
